&i2c1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c1_pmux>;

	vcpu: regulator@62 {
		compatible = "mps,mp8867";
		regulator-name = "vcpu";
		regulator-min-microvolt = <700000>;
		regulator-max-microvolt = <980000>;
		regulator-always-on;
		regulator-boot-on;
		enable-gpios = <&portd 7 GPIO_ACTIVE_LOW>;
		mps,fb-voltage-divider = <80 240>;
		mps,switch-frequency-hz = <1500000>;
		mps,vsel-step = <1>;
		reg = <0x62>;
	};

	vcpu1: regulator1@60 {
		status = "disabled";
		compatible = "silergy,sy8827n";
		regulator-name = "vcpu";
		regulator-min-microvolt = <700000>;
		regulator-max-microvolt = <987500>;
		regulator-always-on;
		regulator-boot-on;
		enable-gpios = <&portd 7 GPIO_ACTIVE_LOW>;
		silergy,vsel-step = <1>;
		reg = <0x60>;
	};

	vcpu2: regulator2@40 {
		status = "disabled";
		compatible = "silergy,syr837_8";
		regulator-name = "vcpu";
		regulator-min-microvolt = <712500>;
		regulator-max-microvolt = <987500>;
		regulator-always-on;
		regulator-boot-on;
		enable-gpios = <&portd 7 GPIO_ACTIVE_LOW>;
		silergy,vsel-step = <1>;
		reg = <0x40>;
	};
};

